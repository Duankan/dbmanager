{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "external-helpers",
    "transform-vue-jsx",
    "jsx-v-model",
    [
      "import",
      {
        "libraryName": "@ktw/kbus",
        "libraryDirectory": "src/components",
        "camel2DashComponentName": false
      },
      {
        "libraryName": "@ktw/kcharts",
        "libraryDirectory": "lib"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "transform-vue-jsx",
        "jsx-v-model",
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}
